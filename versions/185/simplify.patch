-; Sets current test code and optional name. Also resets
-; checksum.
-; Preserved: A, X, Y
-.macro set_test code,name
-	pha
-	lda #code
-	jsr set_test_
-	.ifblank name
-		setb test_name+1,0
-	.else
-		.local Addr
-		setw test_name,Addr
