-	cmp #7
-	bcs :-      ; do multiples of 7
-	lsr a       ; bit 0
-	bcs :+
-:           ; A=clocks/2, either 0,1,2,3
-	beq @zero   ; 0: 5
-	lsr a
-	beq :+      ; 1: 7
